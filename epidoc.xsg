  >:[bef_aft_ex b]"[" [supexpan d] "]"[bef_aft_ex c] = [bef_aft_ex b]<supplied reason="lost">[supexpan d]</>[bef_aft_ex c]
  >:[bef_aft_ex b]"[" [supcertexpan e] "(?)" "]"[bef_aft_ex c] [expanstuff more] = [bef_aft_ex b]<supplied reason="lost" cert="low">[supcertexpan e]</>[bef_aft_ex c][expanstuff more]
  >:[bef_aft_ex b]"[" [supcertexpan e] "(?)" "]"[bef_aft_ex c] = [bef_aft_ex b]<supplied reason="lost" cert="low">[supcertexpan e]</>[bef_aft_ex c]
  >:[WORDVEST b][expanstuff more] = [WORDVEST b][expanstuff more]
